@model IEnumerable<DiplomaThesisSystemMVC.Models.DiplomaThesisTopic>


<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>Index</title>
</head>
<body>

    <table class="table">
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.Name)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Description)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.NumberOfStudents)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.FieldOfStudy)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Degree)
            </th>
            <th></th>
        </tr>

        @foreach (var item in Model)
        {
            <tr>
                <td>
                    @Html.DisplayFor(modelItem => item.Name)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.Description)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.NumberOfStudents)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.FieldOfStudy)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.Degree)
                </td>
            </tr>
        }

    </table>

    @Html.Partial("_SubmitTopicPartial", new DiplomaThesisSystemMVC.Models.DiplomaThesisTopic())

</body>
</html>
